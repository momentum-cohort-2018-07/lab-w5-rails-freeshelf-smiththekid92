<h1>User List</h1>

<%= link_to 'New User', new_user_path %> |
<%= link_to 'Back', books_path %>

<table>
    <tr>
      <th>User:</th>
    </tr>

  <tbody>
    <% @user.each do |user| %>
      <tr>
        <td><%= user.username %></td>
        <td><%= link_to 'Show', user %></td>
        <td><%= link_to 'Edit', edit_user_path(user) %></td>
      </tr>
    <% end %>
  <tbody>
</table>


